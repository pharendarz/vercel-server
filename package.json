  "bugs": {
    "url": "https://github.com/pharendarz/vercel-server/issues"
  },
  "homepage": "https://github.com/pharendarz/vercel-server#readme",
  "dependencies": {
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.3",
    "@types/mongodb": "^3.6.8",
    "express": "^4.19.2",
    "nodemon": "^3.1.0",
    "pre-commit": "^1.2.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rimraf": "^5.0.5",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/node": "^13.9.0",
